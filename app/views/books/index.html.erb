<% for book in @books do %>
  <h2><%=h book.title %></h2>
  <h3><%=h book.subtitle %></h3>
  
  <p><%= book.name %> <%= book.surname %></p>
  <p>ISBN <%= book.isbn %></p>
  <p><%= book.category %></p>
  <p><%= book.color %></p>
  <p><%= book.tags %></p>
  <p><%= book.lease %></p>
  <p><%= book.cover %></p>

<hr>

<% end %>


   